# Configuration Base de Données (I6)
# Par défaut: SQLite local
DB_URL=sqlite:///./data/app.db

# Pour PostgreSQL (production):
# DB_URL=postgresql+psycopg://user:password@localhost:5432/banking_simulator

# Stockage des artifacts
# Options: "db" (BLOB dans la DB) ou "file" (fichiers locaux)
ARTIFACT_STORE=file
ARTIFACT_PATH=./data/artifacts

# Logging
LOG_LEVEL=INFO

